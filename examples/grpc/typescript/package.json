{
  "name": "engram-grpc-client",
  "version": "1.0.0",
  "description": "TypeScript gRPC client for Engram cognitive graph database",
  "main": "dist/engram-client.js",
  "types": "dist/engram-client.d.ts",
  "scripts": {
    "build": "tsc",
    "generate": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:. --grpc_out=grpc_js:. --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ../../proto ../../proto/engram.proto",
    "example": "ts-node engram-client.ts",
    "test": "jest"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "grpc-tools": "^1.12.0",
    "grpc_tools_node_protoc_ts": "^5.3.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}